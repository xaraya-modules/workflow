{
    "name": "xaraya/workflow",
    "type": "xaraya-module",
    "description": "Workflow Module based on the Galaxia Workflow Engine",
    "homepage": "https://github.com/xaraya-modules/workflow",
    "license": "GPL-2.0-or-later",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "xaraya/core": "<2.4.1"
    },
    "autoload": {
        "classmap": ["./"]
    },
    "require": {
        "psr/log": "^3.0",
        "symfony/event-dispatcher": "^7.4",
        "symfony/workflow": "^7.4"
    }
}
